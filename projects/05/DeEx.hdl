/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP DeEx {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=in[0],out=out1);
	Not(in=out1,out=out);
	
}